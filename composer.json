{
    "name": "hypervel/broadcasting",
    "type": "library",
    "description": "The filesystem package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "swoole",
        "broadcasting",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Broadcasting\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "hyperf/contract": "3.2.x-dev",
        "hyperf/http-server": "3.2.x-dev",
        "hyperf/pool": "3.2.x-dev",
        "hyperf/redis": "3.2.x-dev",
        "hypervel/auth": "dev-hyperf3.2",
        "hypervel/bus": "dev-hyperf3.2",
        "hypervel/cache": "dev-hyperf3.2",
        "hypervel/foundation": "dev-hyperf3.2",
        "hypervel/queue": "dev-hyperf3.2",
        "hypervel/router": "dev-hyperf3.2",
        "hypervel/support": "dev-hyperf3.2",
        "hypervel/object-pool": "dev-hyperf3.2"
    },
    "suggest": {
        "ext-hash": "Required to use the Ably and Pusher broadcast drivers.",
        "ext-redis": "Required to use the Redis cache and queue drivers (^4.0|^5.0|^6.0).",
        "ably/ably-php": "Required to use the Ably broadcast driver (^1.0).",
        "pusher/pusher-php-server": "Required to use the Pusher broadcast driver (^6.0|^7.0)."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Broadcasting\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/auth"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/bus"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/cache"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/foundation"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/queue"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/router"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/support"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/object-pool"
        }
    ]
}
